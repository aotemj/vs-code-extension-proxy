{
  "name": "vs-extension-todos",
  "displayName": "vs-extension-todos",
  "description": "a vscode extension of todos",
  "version": "0.0.1",
  "publisher": "losidk",
  "engines": {
    "vscode": "^1.55.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/losidk/vs-code-extension-proxy.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vs-extension-todos.helloWorld",
    "onCommand:vs-extension-todos.askQuestion",
    "onView:vs-extension-todos-sidebar"
  ],
  "main": "out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vs-extension-todos-sidebar-view",
          "title": "vs-extension-todos",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "vs-extension-todos-sidebar-view": [
        {
          "type": "webview",
          "id": "vs-extension-todos-sidebar",
          "name": "VSTodo",
          "icon": "media/checklist.svg",
          "contextualTitle": "VSTodo"
        }
      ]
    },
    "commands": [
      {
        "command": "vs-extension-todos.askQuestion",
        "category": "VSTodo",
        "title": "Ask Question"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "../node_modules/.bin/eslint src --ext ts",
    "test": "node out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.10",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.11",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
